#include<iostream>
#include<string>
using namespace std;
void change(string& ch,int k,int len)
{
	int i = 0; char chars = 0;
	char arr[128] = { -1 };      //因为ASCII码值没有-1,只有0~127
	int sign = 0;
	for (i = ch.find_last_of(ch[k]) + 1; i < len; i++)
	{
		if (arr[(int)chars] != chars)   //字符不相同的话
		{
			arr[(int)chars] = chars;    //将字符对应的ASCII码值(整形数据)作为数组的下标来存放字符
			sign++;
		}
	}
		ch[k] = 'a' + sign;
}
int main()
{
	string ch;
	ch =  "bebcadcceedbcbe" ;
	
	/*getline(cin, ch);*/
	int len = ch.length();
	for (int i = 0; i < len; i++)
	{
		change(ch, i, len);
	}
	cout << ch;
}
